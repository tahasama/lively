{
  "expo": {
    "name": "lively",
    "slug": "lively",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f8f8f8"
    },
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-updates",
        {
          "username": "tahasama"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Lively Chat to access your camera.",
          "microphonePermission": "Allow Lively Chat to access your camera."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#52b6e7"
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tahasama.lively",
      "versionCode": 21,
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "bdc93857-b5ca-43a2-8da0-de853cfbc056"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/bdc93857-b5ca-43a2-8da0-de853cfbc056"
    },
    "owner": "tahasama",
    "scheme": "com.tahasama.lively"
  }
}
